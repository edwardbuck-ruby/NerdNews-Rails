<% if params[:tag_search] %>
  $('#tags-list').html('<%= j render(@tags) %>');
<% else %>
  $('#tags-list').append('<%= j render(@tags) %>');
<% end %>

<% if @tags.next_page %>
  $(".pagination").replaceWith('<%= j paginate(@tags) %>');
<% else %>
  $('.pagination').remove();
<% end %>

<% sleep 1 %>

$("[rel=tooltip]").tooltip()
